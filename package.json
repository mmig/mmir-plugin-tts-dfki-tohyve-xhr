{
  "name": "mmir-plugin-tts-dfki-tohyve-xhr",
  "version": "0.1.0",
  "description": "Cordova plugin for the MMIR framework that adds Text To Speech (TTS) synthesis via DFKI-NLP ToHyVe TTS web services",
  "main": "./www/ttsDFKIToHyVeXhr.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./",
    "update:version": "updateversion ./",
    "update:res": "copycordovascripts res/js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-plugin-tts-dfki-tohyve-xhr.git"
  },
  "keywords": [
    "tts",
    "speech",
    "synthesis",
    "tohyve",
    "nvidia nemo 2.0",
    "xhr"
  ],
  "author": "russa",
  "license": "MIT",
  "cordova": {
    "id": "mmir-plugin-tts-dfki-tohyve-xhr",
    "platforms": []
  },
  "mmir": {
    "compat": {
      "./www/ttsDFKIToHyVeXhr.js": {
        "file": "./www/alt/ttsDFKIToHyVeXhrCompat.js",
        "type": "tts"
      }
    }
  },
  "devDependencies": {
    "mmir-lib": "^6.2.0",
    "mmir-plugin-exports": "^2.4.0"
  }
}
